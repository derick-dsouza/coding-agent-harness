{
  "agent_sdks": {
    "claude-agent-sdk": {
      "api_key_env": "CLAUDE_CODE_OAUTH_TOKEN",
      "models": {
        "opus": "claude-opus-4-5-20251101",
        "sonnet": "claude-sonnet-4-5-20250929",
        "haiku": "claude-haiku-4-5-20250929"
      }
    }
  },
  "task_adapters": {
    "linear": {
      "name": "Linear",
      "description": "Linear project management (requires Linear API key)",
      "cli_command": null,
      "requires_api_key": true,
      "rate_limit": {
        "requests_per_hour": 1500,
        "window_minutes": 60,
        "batch_size": 20
      }
    },
    "github": {
      "name": "GitHub Issues",
      "description": "GitHub Issues for task management (requires gh CLI)",
      "cli_command": "gh",
      "requires_api_key": false,
      "rate_limit": {
        "requests_per_hour": 5000,
        "window_minutes": 60,
        "batch_size": 30
      }
    },
    "beads": {
      "name": "BEADS",
      "description": "BEADS local task management (requires bd CLI)",
      "cli_command": "bd",
      "requires_api_key": false,
      "rate_limit": {
        "requests_per_hour": 0,
        "window_minutes": 0,
        "batch_size": 50
      }
    }
  },
  "defaults": {
    "agent_sdk": "claude-agent-sdk",
    "initializer_model": "opus",
    "coding_model": "sonnet",
    "audit_model": "opus",
    "task_adapter": "linear",
    "max_iterations": null
  }
}
